class Gun 
{
	String gN;
	int nOB;
	public Gun(String gN,int nOB) 
	{
		this.gN=gN;
		this.nOB=nOB;
	}
	public void shoot(){
		for(int i=1;i<=nOB;i++)
		{
			System.out.println("Shooted");
		}
	}
}
class PubG 
{
	public static void main(String[] args) 
	{
		Gun g1=new Gun("pistol",5);
		g1.shoot();
		Gun g2=new Gun("AK-47",16);
		g2.shoot();
	}
}