class StudentDetails 
{
	String name;
	long id;
	String email;
	double marks10th;
	double marks12th;
	public StudentDetails(String name,long id,String email,double marks10th,double marks12th){
			this.name=name;
			this.id=id;
			this.email=email;
			this.marks10th=marks10th;
			this.marks12th=marks12th;
	}
	public void display(){
			System.out.println("<--Student Details of class 12th-->");
			System.out.println("Name:"+this.name);
			System.out.println("id:"+this.id);
			System.out.println("email:"+this.email);
			System.out.println("marks10th:"+this.marks10th);
			System.out.println("marks12th:"+this.marks12th);
	}
	public void display1(){
			System.out.println("<--Student Details of class 10th-->");
			System.out.println("Name:"+this.name);
			System.out.println("id:"+this.id);
			System.out.println("email:"+this.email);
			System.out.println("marks10th:"+this.marks10th);
	}
}
